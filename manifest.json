{
  "manifest_version": 2,
  "name": "emo",
  "version": "0.0.1",
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "tabs",
    "activeTab",
    "nativeMessaging",
    "<all_urls>"
  ],
  "page_action": {
    "browser_style": true,
    "default_title": "emo",
    "default_popup": "popup.html"
  },
  "applications": {
    "gecko": {
      "id": "emo@pyghack2017"
    }
  }
}
